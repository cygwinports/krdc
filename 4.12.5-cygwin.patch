+#ifdef TCP_KEEPINTVL
     optval = m_keepalive.intervalSeconds;
     if (setsockopt(cl->sock, IPPROTO_TCP, TCP_KEEPINTVL, &optval, optlen) < 0) {
         kError(5011) << "setsockopt(TCP_KEEPINTVL)" << strerror(errno);
         return;
     }
+#endif
 
